block pageVars
	- var pageTitle    = "Boxed Layout"
	- var pageSubTitle = "Boxed layout with collapsed navigation"
	- var description  = "The boxed layout has a padding around the main wrapper as well as a background applied to it."
	- var activeMenu   = "boxed"

extends template

append pageConfig
	- var headerType = 'sk-header'
	- var boxedLayout = true
	- var starterKit    = true

append breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Starters Kit"}, {name:"Boxed Layout"}])

//- Include page content in page block
append content
	include ../../contents/starter-kit/starter-page-content.html